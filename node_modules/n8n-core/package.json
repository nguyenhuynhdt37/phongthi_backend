{
  "name": "n8n-core",
  "version": "1.83.1",
  "description": "Core functionality of n8n",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "bin": {
    "n8n-copy-static-files": "./bin/copy-static-files",
    "n8n-generate-translations": "./bin/generate-translations",
    "n8n-generate-metadata": "./bin/generate-metadata"
  },
  "files": [
    "dist",
    "bin",
    "LICENSE.md",
    "LICENSE_EE.md"
  ],
  "devDependencies": {
    "@types/aws4": "^1.5.1",
    "@types/concat-stream": "^2.0.0",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.195",
    "@types/mime-types": "^2.1.0",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "@n8n/typescript-config": "1.2.0"
  },
  "dependencies": {
    "@langchain/core": "0.3.30",
    "@sentry/node": "8.52.1",
    "aws4": "1.11.0",
    "axios": "1.8.2",
    "callsites": "3.1.0",
    "chardet": "2.0.0",
    "concat-stream": "2.0.0",
    "cron": "3.1.7",
    "fast-glob": "3.2.12",
    "file-type": "16.5.4",
    "form-data": "4.0.0",
    "iconv-lite": "0.6.3",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "mime-types": "2.1.35",
    "nanoid": "3.3.8",
    "oauth-1.0a": "2.2.6",
    "p-cancelable": "2.1.1",
    "picocolors": "1.0.1",
    "pretty-bytes": "5.6.0",
    "qs": "6.11.0",
    "ssh2": "1.15.0",
    "uuid": "10.0.0",
    "winston": "3.14.2",
    "xml2js": "0.6.2",
    "zod": "3.24.1",
    "@n8n/client-oauth2": "0.23.0",
    "@n8n/di": "0.4.0",
    "n8n-workflow": "1.82.0",
    "@n8n/config": "1.32.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Jan Oberhauser",
    "email": "jan@n8n.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8n-io/n8n.git"
  },
  "scripts": {
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "pnpm watch",
    "format": "biome format --write .",
    "format:check": "biome ci .",
    "lint": "eslint . --quiet",
    "lintfix": "eslint . --fix",
    "watch": "tsc-watch -p tsconfig.build.json --onCompilationComplete \"tsc-alias -p tsconfig.build.json\"",
    "test": "jest",
    "test:dev": "jest --watch"
  }
}